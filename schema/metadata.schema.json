                "automated_collection": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "data_collectors_organization": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "data_collector_compensation": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
