                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "collection_method": {
                    "anyOf": [
                        {
                            "type": "string",
                            "enum": [
                                "manual",
                                "transform",
                                "generated",
                                "AV",
                                "others"
                            ]
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "data_source": {
                    "anyOf": [
                        {
                            "type": "string",
                            "enum": [
                                "3rdParty",
                                "TDEITools",
                                "InHouse"
                            ]
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "dataset_area": {
                    "anyOf": [
                        {
                            "type": "object",
                            "properties": {
                                "type": {
                                    "type": "string"
                                },
                                "features": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "type": {
                                                "type": "string"
                                            },
                                            "id": {
                                                "type": "string"
                                            },
                                            "properties": {
                                                "type": "object",
                                                "properties": {},
                                                "required": []
                                            },
                                            "geometry": {
                                                "type": "object",
                                                "properties": {
                                                    "type": {
                                                        "type": "string",
                                                        "enum": [
                                                            "MultiPolygon"
                                                        ]
                                                    },
                                                    "coordinates": {
                                                        "type": "array",
                                                        "items": {
                                                            "type": "array",
                                                            "items": {
                                                                "type": "array",
                                                                "minItems": 4,
                                                                "items": {
                                                                    "type": "array",
                                                                    "minItems": 2,
                                                                    "items": {
                                                                        "type": "number"
                                                                    }
                                                                }
                                                            }
                                                        }
                                                    }
