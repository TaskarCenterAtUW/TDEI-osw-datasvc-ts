                                            "type",
                                            "geometry"
                                        ]
                                    }
                                }
                            },
                            "required": [
                                "type",
                                "features"
                            ]
                        },
                        {
                            "type": "object",
                            "properties": {
                                "type": {
                                    "type": "string"
                                },
                                "features": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "type": {
                                                "type": "string"
                                            },
                                            "id": {
                                                "type": "string"
                                            },
                                            "properties": {
                                                "type": "object",
                                                "properties": {},
                                                "required": []
                                            },
                                            "geometry": {
                                                "type": "object",
                                                "properties": {
                                                    "type": {
                                                        "type": "string"
                                                    },
                                                    "coordinates": {
                                                        "type": "array",
                                                        "items": {
                                                            "type": "array",
                                                            "items": {
                                                                "type": "array",
                                                                "items": {
                                                                    "type": "number"
                                                                }
                                                            }
                                                        }
                                                    }
                                                },
                                                "required": [
                                                    "type",
                                                    "coordinates"
                                                ]
                                            }
                                        },
                                        "required": [
                                            "type",
                                            "geometry"
                                        ]
                                    }
                                }
                            },
                            "required": [
                                "type",
                                "features"
                            ]
                        },
                        {
                            "type": "null"
                        }
